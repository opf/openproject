<% content_for :header_tags do %>
  <%= stylesheet_link_tag 'global.css', :media => 'print,screen', :plugin => 'chiliproject_backlogs' %>
  <%= stylesheet_link_tag 'global_print.css', :media => 'print', :plugin => 'chiliproject_backlogs' %>

  <% unless Backlogs::Compatibility.using_jquery? %>
    <%= javascript_include_tag 'lib/jquery-1.5.1.min.js',
                                'lib/jquery-ui-1.8.11.custom.min.js',
                                :plugin => 'chiliproject_backlogs' %>
    <%= javascript_tag "jQuery.noConflict();" %>
    <%= stylesheet_link_tag 'global.css', :media => 'print,screen', :plugin => 'chiliproject_backlogs' %>
  <% end %>

  <%= javascript_include_tag 'lib/jquery.jeditable.mini.js',
                             'lib/jquery.cookie.js',
                             'app/common.js',
                              :plugin => 'chiliproject_backlogs' %>

  <%= javascript_include_tag 'lib/livepipe-ui/livepipe', 'lib/livepipe-ui/window', 'app/modal', :plugin => 'chiliproject_backlogs' %>
  <%= stylesheet_link_tag 'livepipe-ui/base', :plugin => 'chiliproject_backlogs' %>
<% end %>
