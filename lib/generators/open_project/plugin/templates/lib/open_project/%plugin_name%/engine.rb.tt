module OpenProject::<%= plugin_name.camelcase %>
  class Engine < ::Rails::Engine
    engine_name :openproject_<%= plugin_name %>

    include OpenProject::Plugins::ActsAsOpEngine

    register '<%= full_name %>',
             :author_url => 'http://finn.de',
             :requires_openproject => '>= 3.0.0pre13'

  end
end
